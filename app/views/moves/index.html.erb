<p id="notice"><%= notice %></p>

<h1>Moves</h1>

<table class="table table-hover">

  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @moves.each do |move| %>
      <tr>
        <td><%= link_to move.name, move %></td>
        <td><%= move.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

