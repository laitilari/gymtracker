<p id="notice"><%= notice %></p>

<p>
  <strong>Username:</strong>
  <%= @user.username %>
</p>

<p>
  <strong>Admin:</strong>
  <%= @user.admin %>
</p>

<p>
  <strong>Blocked:</strong>
  <%= @user.blocked %>
</p>
<% if false %>
<ul>
  <% @user.trainingmoves.each do |t| %>
  <li>
    <%= t %>
    <% if @user == current_user %>
      <%= link_to 'delete', t, class:"btn btn-danger", method: :delete, data: { confirm: 'Are you sure?' } %>
    <% end %>
  </li>
  <% end %>
</ul>
<% end %>
<%= link_to 'Edit', edit_user_path(@user) %> |
<%= link_to 'Back', users_path %>
