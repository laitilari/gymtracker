<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table class="table table-hover">

  <thead>
    <tr>
      <th>Username</th>
      <th>Latest Result</th>
      <th>Favourite Move</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.username, user %></td>
    <% end %>
  </tbody>
</table>

<br>
